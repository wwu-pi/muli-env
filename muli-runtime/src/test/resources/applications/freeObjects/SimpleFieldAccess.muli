package applications.freeObjects;
public class SFAA {
    public int i = 2;
}
public class SFAB extends SFAA {
    public int i = 1;
}
public class SimpleFieldAccess {
    public static void main(String[] args) {
        SFAA a = new SFAA();
        SFAA b = new SFAB();
        System.out.print(a.i);
        System.out.print(".");
        System.out.println(b.i);
    }
}
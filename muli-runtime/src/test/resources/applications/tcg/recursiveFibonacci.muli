package applications.tcg;

import java.util.List;
import de.wwu.muli.*;
import de.wwu.muli.solution.*;
import java.lang.Object;

public class Main {

    public static void main(String[] args) {
        Solution[] solutions = Muli.getAllSolutionsWithTestCases(Fibonacci::searchRegion, Fibonacci.class.getName() + ".fibonacci");
        for(Solution s : solutions){
            System.out.println("output: "+s.value);
            TestCase<?> tc = s.testCase;
            Object[] inputs = tc.getInputs();
            for(Object o : inputs) {
                System.out.println("input: "+o.toString());
            }
        }
    }

}

class Fibonacci {
    static int fibonacci(int n) {
        if (n < 1) {
          return 0;
        } else if (n == 1) {
          return 1;
        } else {
          return fibonacci(n-1) + fibonacci(n-2);
        }
    }

    static int searchRegion() {
        int x = Verifier.nondetInt();
        if (x > 10 || x < 0) {
          throw Muli.fail();
        }
        int result = fibonacci(x);
        return result;
    }
}

public class Verifier{
    public static int nondetInt() {
        int i free;
        return i;
      }
}
    }

    private static Boolean check() {
        int[] vals free;
        int i free;
        int val free;
        int notVal = 0;

        if (vals.length != 2) {
            throw Muli.fail();
        }
        if (vals[0] == notVal) {
            throw Muli.fail();
        }
        if (vals[1] == notVal) {
            throw Muli.fail();
        }

        return vals[i] == notVal;
    }
}
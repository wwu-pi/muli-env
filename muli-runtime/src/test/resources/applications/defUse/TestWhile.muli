public class TestWhile {

    public static void main(String[] args) {
        Solution[] solution = Muli.getAllSolutionsWithTestCases(TestWhile::searchRegion, TestWhile.class.getName() + ".whileTest");
    }

    static int searchRegion(){
            int[] x free;
            if(x.length < 2 || x.length >3) Muli.fail();
            int[] y = whileTest(x);
            return y[0];
        }
        static int[] whileTest(int[] x){
            while(x[0] != 5){
                x[0] = 5;
                if(x[1] != 2){
                    x[1] = 2;
                }
            }
            return x;
        }

}
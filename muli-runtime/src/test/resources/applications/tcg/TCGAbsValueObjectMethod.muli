package applications.tcg;

import de.wwu.muli.*;
import de.wwu.muli.solution.*;
import java.lang.Object;
import de.wwu.muli.tcg.*;

public class TCGAbsValueObjectMethod {

    public static void main(String[] args) {
        Solution[] solutions = Muli.getAllSolutionsWithTestCases(DriverClass::driver, TCGAbsValueObjectMethod.class.getName() + ".abs");
        TestCaseGenerator tcg = TestCaseGenerator.get(solutions);
        tcg.generateTestClassStringRepresentation();
    }
    public int abs(int i) {
        if (i < 0) {
            return -i;
        } else {
            return i;
        }
    }

}
public class DriverClass {
    public static int driver() {
        int i free;
        TCGAbsValueObjectMethod obj = new TCGAbsValueObjectMethod();
        return obj.abs(i);
    }
}
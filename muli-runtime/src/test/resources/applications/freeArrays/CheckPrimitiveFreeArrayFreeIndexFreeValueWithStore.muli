        Solution<Object>[] solutions = Muli.getAllSolutions(CheckPrimitiveFreeArrayFreeIndexFreeValueWithStore::check);
    }

    private static Boolean check() {
        int[] vals free;
        int i free;
        int val free;

        if (i >= vals.length || i < 0) {
            throw Muli.fail();
        }
        vals[i] = val;
        if (i != 3) {
            throw Muli.fail();
        }
        return vals[3] == val;
